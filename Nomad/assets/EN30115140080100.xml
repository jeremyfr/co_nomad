    <SUBTASK CHAPNBR="30" CHG="U" CONFLTR="A" CONFNBR="00" FUNC="410" KEY="EN30115141000100001" PGBLKNBR="04" REVDATE="20080701" SECTNBR="11" SEQ="001" SUBJNBR="51">
